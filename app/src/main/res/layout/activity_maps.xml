<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MapsActivity" />

    <android.support.v7.widget.CardView
        android:id="@+id/card_btnNewsFeed"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        app:cardBackgroundColor="@color/colorWhite"
        app:cardCornerRadius="5dp">

        <ImageView
            android:id="@+id/btnNewsFeed"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorWhite"
            android:src="@drawable/ic_news_feed"
            android:padding="5dp"
            />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_btnProfile"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/card_btnNewsFeed"
        app:cardBackgroundColor="@color/colorWhite"
        app:cardCornerRadius="5dp">

        <ImageView
            android:id="@+id/btnProfile"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorWhite"
            android:src="@drawable/ic_profile_main"
            android:padding="5dp"
            />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_btnFriends"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/card_btnProfile"
        app:cardBackgroundColor="@color/colorWhite"
        app:cardCornerRadius="5dp">

        <ImageView
            android:id="@+id/btnFriends"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorWhite"
            android:src="@drawable/ic_friends"
            android:padding="5dp"
            />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/card_btnNotification"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/card_btnFriends"
        app:cardBackgroundColor="@color/colorWhite"
        app:cardCornerRadius="5dp">

        <ImageView
            android:id="@+id/btnNotification"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorWhite"
            android:src="@drawable/ic_notification"
            android:padding="5dp"
            />
    </android.support.v7.widget.CardView>

    <Button
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/rounded_button"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="20dp"
        android:text="Tattle"
        android:textColor="#1bbc9b"

        />

</RelativeLayout>